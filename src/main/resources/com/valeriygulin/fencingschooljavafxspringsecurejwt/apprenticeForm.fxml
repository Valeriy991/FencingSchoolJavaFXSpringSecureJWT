                prefHeight="26.0" prefWidth="474.0" styleClass="warning" text="Удалить тренировку"/>
        <Label layoutX="196.0" layoutY="34.0" text="Расписание тренировок"/>
        <ListView fx:id="listViewTrainings" layoutX="196.0" layoutY="59.0" prefHeight="343.0" prefWidth="474.0"/>
        <TextField fx:id="textFieldPassword" layoutX="30.0" layoutY="130.0"/>
        <TextField fx:id="textFieldLogin" layoutX="30.0" layoutY="68.0"/>
        <Label layoutX="27.0" layoutY="43.0" text="Логин"/>
        <Label layoutX="27.0" layoutY="106.0" text="Пароль"/>
    </children>
</AnchorPane>
